# S&R Bots Simulator default configuration

# configuration file for the simulated environment (path relative to top package directory)
envConfigFileName=usr/conf/default.ConfigEnv

# comma-separated list of agent configuration files (path relative to top package directory)
objectConfigFileNames=usr/conf/line.Defender,usr/conf/default.Attacker,usr/conf/default.Flag1,usr/conf/default.Obstacle

# Number of teams
numberOfTeams=2


